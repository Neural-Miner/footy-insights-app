{
  "name": "footy-insights-app",
  "homepage": "https://neural-miner.github.io/footy-insights-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cra-template": "1.2.0",
    "framer-motion": "^12.7.4",
    "gh-pages": "^6.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "client": "react-scripts start",
    "server": "python3 backend/server.py",
    "start": "concurrently \"cd ../ && python3 -m backend.server\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "tailwindcss-cli": "^0.1.2"
  }
}
